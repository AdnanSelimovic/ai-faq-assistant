APP_NAME=Laravel
APP_ENV=local
# APP_KEY is required in production; set it in Railway (do not commit).
APP_KEY=
APP_DEBUG=true
# APP_URL should be your public URL in production.
APP_URL=http://localhost
# SINGLE_USER_EMAIL is required for login access in production.
SINGLE_USER_EMAIL=admin@example.com

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

# PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

# Optional LLM mode (set OPENAI_API_KEY to enable).
ASK_ANSWER_MODE_DEFAULT=extractive
ASK_MAX_CONTEXT_CHUNKS=5
ASK_MAX_CONTEXT_CHARS=4000
# Legacy name (not used by config; keep for reference).
ASK_OPENAI_MAX_OUTPUT_TOKENS=400
OPENAI_API_KEY=
OPENAI_MODEL=gpt-4o-mini
OPENAI_STORE=false
# Current config expects this name.
OPENAI_MAX_OUTPUT_TOKENS=400

# Database (Railway MySQL: set DB_CONNECTION=mysql and DB_* values).
DB_CONNECTION=sqlite
# DB_HOST=127.0.0.1
# DB_PORT=3306
# DB_DATABASE=laravel
# DB_USERNAME=root
# DB_PASSWORD=

SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local
QUEUE_CONNECTION=database

CACHE_STORE=database
# CACHE_PREFIX=

MEMCACHED_HOST=127.0.0.1

REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=log
MAIL_SCHEME=null
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

VITE_APP_NAME="${APP_NAME}"
